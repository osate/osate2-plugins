package partition
public
	with ARINC653;
	
	system s1
	end s1;
	
	system implementation s1.i1
		subcomponents
			sub1: system s2;
			sub2: abstract a1;
			sub3: virtual processor {Period => 1 ms;};
			sub4: system s2;
			sub5: virtual processor {Period => 1 ms;};
			sub6: processor;
			sub7: abstract a2;
			sub8: abstract a2;
			sub9: system s3;
			sub10: abstract a2;
			sub11: abstract a1;
			sub12: virtual processor;
			sub13: processor {ARINC653::Module_Major_Frame => 1 ms;};
			sub14: abstract a1;
			sub15: virtual processor {ARINC653::Module_Major_Frame => 1 ms;};
			sub16: abstract a1;
			sub17: virtual processor vp1.i {Period => 1 ms;};
			sub18: abstract a1;
			sub19: virtual processor;
		connections
			conn1: feature sub1.f1 -> sub2.f2;
			conn2: feature sub4.f1 -> sub2.f2;
			conn3: feature sub7.f3 -> sub2.f2;
			conn4: feature sub8.f3 -> sub2.f2;
			conn5: feature sub1.f1 -> sub9.f4;
			conn6: feature sub10.f3 -> sub9.f4;
			conn7: feature sub4.f1 -> sub9.f4;
			conn8: feature sub1.f1 -> sub11.f2;
			conn9: feature sub1.f1 -> sub14.f2;
			conn10: feature sub1.f1 -> sub16.f2;
			conn11: feature sub1.f1 -> sub18.f2;
		flows
			etef1: end to end flow sub1.fsource1 -> conn1 -> sub2.fsink1;
			etef2: end to end flow sub4.fsource1 -> conn2 -> sub2.fsink1;
			etef3: end to end flow sub7.fsource2 -> conn3 -> sub2.fsink1;
			etef4: end to end flow sub8.fsource2 -> conn4 -> sub2.fsink1;
			etef5: end to end flow sub1.fsource1 -> conn5 -> sub9.fsink2;
			etef6: end to end flow sub10.fsource2 -> conn6 -> sub9.fsink2;
			etef7: end to end flow sub4.fsource1 -> conn7 -> sub9.fsink2;
			etef8: end to end flow sub1.fsource1 -> conn8 -> sub11.fsink1;
			etef9: end to end flow sub1.fsource1 -> conn9 -> sub14.fsink1;
			etef10: end to end flow sub1.fsource1 -> conn10 -> sub16.fsink1;
			etef11: end to end flow sub1.fsource1 -> conn11 -> sub18.fsink1;
		properties
			Actual_Processor_Binding => (reference (sub3)) applies to sub1;
			Actual_Processor_Binding => (reference (sub5)) applies to sub4;
			Actual_Processor_Binding => (reference (sub6)) applies to sub5;
			ARINC653::Module_Schedule => ([
				Partition => reference (sub5);
				Duration => 1 ms;
			]) applies to sub6;
			Actual_Processor_Binding => (reference (sub3)) applies to sub7;
			Actual_Processor_Binding => (reference (sub5)) applies to sub8;
			Actual_Processor_Binding => (reference (sub3)) applies to sub9;
			Actual_Processor_Binding => (reference (sub12)) applies to sub11;
			Actual_Processor_Binding => (reference (sub13)) applies to sub12;
			Actual_Processor_Binding => (reference (sub15)) applies to sub14;
			Actual_Processor_Binding => (reference (sub17.sub20)) applies to sub16;
			Actual_Processor_Binding => (reference (sub19)) applies to sub18;
	end s1.i1;
	
	system s2
		features
			f1: feature;
		flows
			fsource1: flow source f1;
		properties
			Period => 1 ms;
	end s2;
	
	abstract a1
		features
			f2: feature;
		flows
			fsink1: flow sink f2;
	end a1;
	
	abstract a2
		features
			f3: feature;
		flows
			fsource2: flow source f3;
	end a2;
	
	system s3
		features
			f4: feature;
		flows
			fsink2: flow sink f4;
	end s3;
	
	virtual processor vp1
	end vp1;
	
	virtual processor implementation vp1.i
		subcomponents
			sub20: virtual processor;
	end vp1.i;
end partition;